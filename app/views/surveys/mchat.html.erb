<!DOCTYPE html>
<html>
    <head>
      <meta charset='utf-8' />
      <%= wicked_pdf_stylesheet_link_tag "vaccines_pdf" %>
      <%= wicked_pdf_stylesheet_link_tag "surveys_pdf" %>
    </head>

    <body>
        <% start, page_count, questions = 1, 1, survey.questions %>
        <%= Tilt::ERBTemplate.new(Rails.root.join('app', 'views', 'surveys', 'mchat_first_page.erb')).render(self, count: page_count, records: questions[0..3])%>
        <% page_count += 1; start += 3 %>
        <%= Tilt::ERBTemplate.new(Rails.root.join('app', 'views', 'surveys', 'mchat_last_page.erb')).render(self, count: page_count, start: start, records: questions[4..11])%>
        <% page_count += 1; start += 8 %>
        <%= Tilt::ERBTemplate.new(Rails.root.join('app', 'views', 'surveys', 'mchat_last_page.erb')).render(self, count: page_count, start: start, records: questions[12..19])%>
    </body>
</html>
